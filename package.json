        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-node": "^11.1.0",
        "mocha": "^10.0.0"
    },
    "peerDependencies": {
        "eslint": ">=7"
    },
    "engines": {
        "node": "^14.17.0 || ^16.0.0 || >= 18.0.0"
    }
